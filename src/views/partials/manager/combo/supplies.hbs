<table class="table table-hover">
  <thead>
    <tr>
      <th>Barcode</th>
      <th>Name</th>
      <th>Cant.</th>
      <th>$ Prices</th>
    </tr>
  </thead>
  <tbody>
    {{#each suppliesCombo}}
        <tr>
            <td>{{product_barcode}}</td>
            <td>{{product_name}}</td>
            <td>{{amount}}/{{unity}}</td>
            <td>{{sale_price}}/{{currency}}$</td>
        </tr>
    {{/each}}
  </tbody>
</table>